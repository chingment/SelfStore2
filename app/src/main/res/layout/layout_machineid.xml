<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/layout_machineid"
    android:layout_marginTop="@dimen/size_45"
    android:layout_width="@dimen/size_180"
    android:layout_height="@dimen/size_40"
    android:layout_marginBottom="@dimen/size_4"
    android:gravity="center"
    android:background="@drawable/layout_machineid_bg">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_gravity="center"
            android:gravity="center"
            >
        <TextView
            android:id="@+id/tv_machineId_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="设备编码:"
            ></TextView>
        <TextView
            android:id="@+id/tv_machineId_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text=""
            ></TextView>
</LinearLayout>


</RelativeLayout>

